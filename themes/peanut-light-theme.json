	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#8a8175"
			}
		},
		{
			"name": "Comments: Preprocessor",
			"scope": "comment.block.preprocessor",
			"settings": {
				"fontStyle": "",
				"foreground": "#8a8175"
			}
		},
		{
			"name": "Comments: Documentation",
			"scope": [
				"comment.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#75716a"
			}
		},
		{
			"name": "Invalid - Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"foreground": "#d63447"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#6b6762"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#c15f3c"
			}
		},
		{
			"name": "Storage (var, const, let, function, class)",
			"scope": [
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#c15f3c"
			}
		},
		{
			"name": "Types",
			"scope": [
				"entity.name.type",
				"support.type",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#d97d28"
			}
		},
		{
			"name": "Language Constants (true, false, null, undefined)",
			"scope": [
				"constant.language",
				"support.constant"
			],
			"settings": {
				"foreground": "#b86828"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"support.variable"
			],
			"settings": {
				"foreground": "#2b6a8e"
			}
		},
		{
			"name": "Variable: this, super",
			"scope": [
				"variable.language"
			],
			"settings": {
				"foreground": "#b86828",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Variable: Object properties",
			"scope": [
				"variable.other.property",
				"variable.other.object.property",
				"support.variable.property"
			],
			"settings": {
				"foreground": "#3d7fa5"
			}
		},
		{
			"name": "Variable: Function parameters",
			"scope": [
				"variable.parameter",
				"meta.parameters variable"
			],
			"settings": {
				"foreground": "#5d8f2f",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#d63447"
			}
		},
		{
			"name": "Classes",
			"scope": [
				"entity.name.type.class",
				"support.class"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#d97d28"
			}
		},
		{
			"name": "Interfaces",
			"scope": [
				"entity.name.type.interface"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#b86828"
			}
		},
		{
			"name": "Exceptions",
			"scope": "entity.name.exception",
			"settings": {
				"foreground": "#d63447"
			}
		},
		{
			"name": "Sections",
			"scope": "entity.name.section",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Numbers",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#b86828"
			}
		},
		{
			"name": "Characters and Constants",
			"scope": [
				"constant.character",
				"constant"
			],
			"settings": {
				"foreground": "#b86828"
			}
		},
		{
			"name": "Strings",
			"scope": "string",
			"settings": {
				"foreground": "#4d7c25"
			}
		},
		{
			"name": "Strings: Escape Sequences",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#6b6762"
			}
		},
		{
			"name": "Strings: Regular Expressions",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#2b6a8e"
			}
		},
		{
			"name": "Strings: Symbols",
			"scope": "constant.other.symbol",
			"settings": {
				"foreground": "#b86828"
			}
		},
		{
			"name": "Template Strings",
			"scope": [
				"string.template",
				"punctuation.definition.string.template"
			],
			"settings": {
				"foreground": "#5d8f2f"
			}
		},
		{
			"name": "Template Expression",
			"scope": [
				"meta.template.expression"
			],
			"settings": {
				"foreground": "#2d2a26"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation",
			"settings": {
				"foreground": "#6b6762"
			}
		},
		{
			"name": "Imports and Exports",
			"scope": [
				"keyword.control.import",
				"keyword.control.export",
				"keyword.control.from"
			],
			"settings": {
				"foreground": "#c15f3c",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Module Names in Imports",
			"scope": [
				"variable.other.readwrite.alias",
				"meta.import variable",
				"meta.export variable"
			],
			"settings": {
				"foreground": "#d97d28"
			}
		},
		{
			"name": "JSON Keys",
			"scope": [
				"support.type.property-name.json",
