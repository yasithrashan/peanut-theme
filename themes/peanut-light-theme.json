	"semanticHighlighting": true,
	"semanticTokenColors": {
		"variable": "#2b6a8e",
		"variable.readonly": "#3d7fa5",
		"variable.declaration": "#2b6a8e",
		"parameter": {
			"foreground": "#5d8f2f",
			"fontStyle": "italic"
		},
		"property": "#3d7fa5",
		"property.readonly": "#4a8fb8",
		"function": {
			"foreground": "#d63447",
			"fontStyle": "italic"
		},
		"method": {
			"foreground": "#d63447",
			"fontStyle": "italic"
		},
		"class": {
			"foreground": "#d97d28",
			"fontStyle": "italic"
		},
		"interface": {
			"foreground": "#b86828",
			"fontStyle": "italic"
		},
		"type": "#d97d28",
		"enum": "#d97d28",
		"enumMember": "#b86828",
		"namespace": "#d97d28",
		"keyword": "#c15f3c",
		"modifier": "#c15f3c",
		"string": "#4d7c25",
		"number": "#b86828",
		"regexp": "#2b6a8e",
		"operator": "#6b6762",
		"comment": {
			"foreground": "#8a8175",
			"fontStyle": "italic"
		}
	}
