			"settings": {
				"foreground": "#4d7c25"
			}
		},
		{
			"name": "CSS: Important Keyword",
			"scope": "keyword.other.important",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#d63447"
			}
		},
		{
			"name": "Markdown: Headings",
			"scope": [
				"markup.heading",
				"entity.name.section.markdown"
			],
			"settings": {
				"foreground": "#d63447",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown: Bold",
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#2d2a26"
			}
		},
		{
			"name": "Markdown: Italic",
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#2d2a26"
			}
		},
		{
			"name": "Markdown: Link Text",
