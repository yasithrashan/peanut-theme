				"support.class.component.jsx",
				"support.class.component.tsx"
			],
			"settings": {
				"foreground": "#d97d28",
				"fontStyle": "bold"
			}
		},
		{
			"name": "CSS: Selectors",
			"scope": [
				"meta.selector",
				"meta.selector entity",
				"meta.selector entity punctuation",
				"entity.name.tag.css"
			],
			"settings": {
				"foreground": "#c15f3c"
			}
		},
		{
			"name": "CSS: Property Names",
			"scope": [
				"meta.property-name",
				"support.type.property-name"
