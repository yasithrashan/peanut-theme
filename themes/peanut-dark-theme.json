	"semanticHighlighting": true,
	"semanticTokenColors": {
		"variable": "#6AADC5",
		"variable.readonly": "#7DC5DD",
		"variable.declaration": "#6AADC5",
		"parameter": {
			"foreground": "#8FB544",
			"fontStyle": "italic"
		},
		"property": "#7DC5DD",
		"property.readonly": "#8DD5ED",
		"function": {
			"foreground": "#FF8C6F",
			"fontStyle": "italic"
		},
		"method": {
			"foreground": "#FF8C6F",
			"fontStyle": "italic"
		},
		"class": {
			"foreground": "#FFAA66",
			"fontStyle": "italic"
		},
		"interface": {
			"foreground": "#FFA459",
			"fontStyle": "italic"
		},
		"type": "#FFAA66",
		"enum": "#FFAA66",
		"enumMember": "#FFA459",
		"namespace": "#FFAA66",
		"keyword": "#D099D4",
		"modifier": "#D099D4",
		"string": "#8FB544",
		"number": "#FFA459",
		"regexp": "#6AADC5",
		"operator": "#B5A595",
		"comment": {
			"foreground": "#9A8A7A",
			"fontStyle": "italic"
		}
	}
}
