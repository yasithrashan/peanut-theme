	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#9A8A7A"
			}
		},
		{
			"name": "Comments: Preprocessor",
			"scope": "comment.block.preprocessor",
			"settings": {
				"fontStyle": "",
				"foreground": "#9A8A7A"
			}
		},
		{
			"name": "Comments: Documentation",
			"scope": [
				"comment.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#B5A595"
			}
		},
		{
			"name": "Invalid - Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"foreground": "#FF6F54"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#B5A595"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#D099D4"
			}
		},
		{
			"name": "Storage (var, const, let, function, class)",
			"scope": [
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#D099D4"
			}
		},
		{
			"name": "Types",
			"scope": [
				"entity.name.type",
				"support.type",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#FFAA66"
			}
		},
		{
			"name": "Language Constants (true, false, null, undefined)",
			"scope": [
				"constant.language",
				"support.constant"
			],
			"settings": {
				"foreground": "#FFA459"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"support.variable"
			],
			"settings": {
				"foreground": "#6AADC5"
			}
		},
		{
			"name": "Variable: this, super",
			"scope": [
				"variable.language"
			],
			"settings": {
				"foreground": "#FFA459",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Variable: Object properties",
			"scope": [
				"variable.other.property",
				"variable.other.object.property",
				"support.variable.property"
			],
			"settings": {
				"foreground": "#7DC5DD"
			}
		},
		{
			"name": "Variable: Function parameters",
			"scope": [
				"variable.parameter",
				"meta.parameters variable"
			],
			"settings": {
				"foreground": "#8FB544",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FF8C6F"
			}
		},
		{
			"name": "Classes",
			"scope": [
				"entity.name.type.class",
				"support.class"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FFAA66"
			}
		},
		{
			"name": "Interfaces",
			"scope": [
				"entity.name.type.interface"
